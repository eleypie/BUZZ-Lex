// Literals
TOKEN:  221                  TYPE: INTEGER
TOKEN:  1.5                  TYPE: FLOAT
TOKEN:  5                    TYPE: INTEGER
TOKEN:  2                    TYPE: INTEGER
TOKEN:  7                    TYPE: INTEGER
TOKEN:  string               TYPE: STRING

// Comments
TOKEN:  <|comment:>          TYPE: COMMENT

// Noise Words
TOKEN:  returnvalue          TYPE: NOISE_WORD

