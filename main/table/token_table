// Identifiers
TOKEN:  #myvariable          TYPE: VAR_IDENT
TOKEN:  #myvariable6         TYPE: VAR_IDENT
TOKEN:  #secretValue         TYPE: VAR_IDENT
TOKEN:  #adelina123          TYPE: VAR_IDENT
TOKEN:  #hello               TYPE: VAR_IDENT
TOKEN:  #name                TYPE: VAR_IDENT
TOKEN:  #a                   TYPE: VAR_IDENT
TOKEN:  #b                   TYPE: VAR_IDENT
TOKEN:  #c                   TYPE: VAR_IDENT

// Function Identifiers
TOKEN:  ~function            TYPE: FUNC_IDENT
TOKEN:  ~NameFunc            TYPE: FUNC_IDENT
TOKEN:  ~cal                 TYPE: FUNC_IDENT

// Operators
TOKEN:  +                    TYPE: ADDITION
TOKEN:  -                    TYPE: SUBTRACTION
TOKEN:  *                    TYPE: MULTIPLICATION
TOKEN:  /                    TYPE: DIVISION
TOKEN:  %                    TYPE: MODULO
TOKEN:  ^                    TYPE: EXPONENT
TOKEN:  =                    TYPE: ASSIGNMENT_OP
TOKEN:  <                    TYPE: LESS_THAN
TOKEN:  >                    TYPE: GREATER_THAN
TOKEN:  >=                   TYPE: GREATER_EQUAL
TOKEN:  <=                   TYPE: LESS_EQUAL
TOKEN:  ==                   TYPE: IS_EQUAL_TO
TOKEN:  !=                   TYPE: NOT_EQUAL
TOKEN:  &&                   TYPE: AND
TOKEN:  ||                   TYPE: OR
TOKEN:  !                    TYPE: NOT

// Delimiters
TOKEN:  (                    TYPE: LEFT_PAREN
TOKEN:  )                    TYPE: RIGHT_PAREN
TOKEN:  [                    TYPE: LEFT_BRACKET
TOKEN:  ]                    TYPE: RIGHT_BRACKET
TOKEN:  {                    TYPE: LEFT_BRACE
TOKEN:  }                    TYPE: RIGHT_BRACE
TOKEN:  ;                    TYPE: SEMICOLON
TOKEN:  ,                    TYPE: COMMA
TOKEN:  "                    TYPE: DBL_QUOTE
TOKEN:  '                    TYPE: SNGL_QUOTE

// Keywords
TOKEN:  buzz                 TYPE: BUZZ_TOKEN
